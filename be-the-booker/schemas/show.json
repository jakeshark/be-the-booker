{
  "$id": "schema/show.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Show",
  "type": "object",
  "required": [
    "id",
    "brand_profile",
    "venue_quality",
    "production_level",
    "crowd_hotness_base",
    "network_morality",
    "card_time_cap",
    "seed"
  ],
  "properties": {
    "id": {
      "type": "string"
    },
    "brand_profile": {
      "type": "number",
      "minimum": 0,
      "maximum": 100
    },
    "venue_quality": {
      "type": "number",
      "minimum": 0,
      "maximum": 100
    },
    "production_level": {
      "type": "number",
      "minimum": 0,
      "maximum": 100
    },
    "crowd_hotness_base": {
      "type": "number",
      "minimum": 0,
      "maximum": 100
    },
    "network_morality": {
      "type": "number",
      "minimum": 0,
      "maximum": 100
    },
    "card_time_cap": {
      "type": "integer",
      "minimum": 1
    },
    "announcers": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "region": {
      "type": "string"
    },
    "variety_memory": {
      "type": "object",
      "properties": {
        "recent_matchups": {
          "type": "array",
          "items": {
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        },
        "recent_stips": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "recent_types": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      }
    },
    "seed": {
      "type": "integer"
    }
  }
}